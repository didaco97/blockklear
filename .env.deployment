# Deployment Environment Variables
# Copy this to .env and fill in your actual values

# Your wallet private key (NEVER commit this to git!)
PRIVATE_KEY=your_wallet_private_key_here

# Sepolia RPC URL (you can use public endpoints or get API keys)
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID
# Alternative public RPC: https://rpc.sepolia.org

# Etherscan API key for contract verification (optional but recommended)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Frontend environment variables
VITE_CONTRACT_ADDRESS=will_be_filled_after_deployment
VITE_ETHEREUM_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID
